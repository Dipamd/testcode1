#include<stdio.h>
#include<conio.h>
void main()
{
   int per;

   printf("\n Enter Percentage :- ");
   scanf("%d",&per);
   if (per>=35 && per<=49)
   {
       printf("\n pass");
   }
   else if  (per>=50 && per<=59)
   {
       printf("\n second class");
   }
   else if (per>=60 && per<=64)
   {
       printf("\n first class");
   }
   else if (per>=65 && per<=100)
   {
       printf("\n first class with distingshion");
   }
   else
   {
       printf("\n candidate is fail");
   }
   return 0;
}
